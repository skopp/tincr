  "version": "1",
  "devtools_page": "devtools.html",
  "background_page": "background.html",
  "icons": {"48": "tools_small.png",
            "128": "tools.png" },
  "permissions": ["http://*/", "https://*/"],
  "plugins": [
    { "path": "NPAPIFileIOforChrome.plugin", "public": false },
    { "path": "npNPAPIFileIOforChrome.dll", "public": false },
    { "path": "npNPAPIFileIOforChrome.so", "public": false },
    { "path": "npNPAPIFileIOforChromex64.so", "public": false }
  ]
}
